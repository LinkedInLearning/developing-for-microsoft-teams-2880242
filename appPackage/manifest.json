        "termsOfUseUrl": "https://localhost:3000/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "picbot2",
        "full": ""
    },
    "description": {
        "short": "Short description for Conversation Bot.",
        "full": "Full description of Conversation Bot."
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "7c145f02-d990-4084-83de-b0453764063b",
