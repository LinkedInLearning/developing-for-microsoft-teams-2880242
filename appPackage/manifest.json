{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.8/MicrosoftTeams.schema.json",
    "manifestVersion": "1.8",
    "version": "1.0.0",
    "id": "e8b5c3ef-0376-4cc9-8143-c06a877a5a12",
    "packageName": "com.picturematic",
    "developer": {
        "name": "Picturematic, Inc.",
        "websiteUrl": "https://localhost:3000",
        "privacyUrl": "https://localhost:3000/privacy",
        "termsOfUseUrl": "https://localhost:3000/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "categorytab",
        "full": ""
    },
    "description": {
        "short": "Short description for My Group Tab.",
        "full": "Full description of My Group Tab."
    },
    "accentColor": "#FFFFFF",
    "configurableTabs": [
        {
            "configurationUrl": "https://localhost:3000/config",
            "canUpdateConfiguration": true,
            "scopes": [
                "team",
                "groupchat"
            ]
        }
    ],
    "staticTabs": [
        {
            "entityId": "business",
            "name": "Picturematic",
            "contentUrl": "https://localhost:3000/tabpersonal",
            "websiteUrl": "https://www.picturematic.com/Catalogue.html",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "about",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "localhost",
        "localhost:3000",
        "www.picturematic.com"
    ]
}